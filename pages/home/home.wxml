<!--pages/home/home.wxml-->
<!--顶部展示栏-->
<view class="center">
  <view class="swiper">
<swiper indicator-dots="{{indicatorDots}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}">
        <block wx:for="{{background}}" wx:key="*this">         
         <swiper-item>        
        <image src="{{item}}" mode="widthFix" ></image>       
        </swiper-item>   
       </block>
</swiper>
</view>
</view>
